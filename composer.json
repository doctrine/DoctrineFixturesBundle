{
    "name": "symfony/doctrine-fixtures-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 Doctrine Fixtures Bundle",
    "keywords": ["database", "fixtures", "symfony"],
    "homepage": "http://www.doctrine-project.org",
    "license": "MIT",
    "authors": [
        {"name": "Fabien Potencier", "email": "fabien@symfony.com"},
        {"name": "Symfony Community", "homepage": "http://symfony.com/contributors"}
    ],
    "require": {
        "php": ">=5.3.2",
        "doctrine/data-fixtures": "*",
        "symfony/doctrine-bundle": "*",
    },
    "autoload": {
        "psr-0": {
            "Symfony\\Bundle\\DoctrineFixturesBundle": ""
        }
    },
    "target-dir" : "Symfony/Bundle/DoctrineFixturesBundle"
}
